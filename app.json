{
    "name": "Lavalink On Heroku",
    "description": "A Heroku app to deploy your own Lavalink server.",
    "keywords": [
      "Lavalink",
      "lavalink",
      "discord-music-streaming",
      "LavalinkOnHeroku",
      "lavalink-on-heroku"
    ],
    "repository": "https://github.com/IamTheRealSami/LavalinkOnHeroku",
    "env": {
      "PASS": {
        "description": "Set this for authorization for your lavalink server. (default: youshallnotpass)",
        "value": "youshallnotpass"
      }
    },
    "labs": [
      "runtime-dyno-metadata"
    ],
    "buildpacks": [
      {
        "url": "heroku/nodejs"
      },
      {
        "url": "heroku/java"
      }
    ]
  }
